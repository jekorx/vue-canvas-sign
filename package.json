{
  "name": "vue-canvas-sign",
  "version": "2.0.2",
  "description": "vue canvas签名组件（vue canvas sign component），with vue@3.x",
  "author": "jekorx",
  "files": [
    "lib",
    "types"
  ],
  "main": "./lib/vue-canvas-sign.umd.js",
  "module": "./lib/vue-canvas-sign.es.js",
  "exports": {
    ".": {
      "import": "./lib/vue-canvas-sign.es.js",
      "require": "./lib/vue-canvas-sign.umd.js"
    }
  },
  "typings": "./types/index.d.ts",
  "private": false,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && eslint src --ext .ts,.js,.tsx,.jsx,.vue && vite build",
    "build:show": "vue-tsc --noEmit && eslint src --ext .ts,.js,.tsx,.jsx,.vue && vite build -c vite.config.show.ts",
    "serve": "vite preview"
  },
  "peerDependencies": {
    "vue": "^3.0.5"
  },
  "devDependencies": {
    "@types/node": "^15.0.1",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "@vitejs/plugin-legacy": "^1.3.2",
    "@vitejs/plugin-vue": "^1.2.2",
    "@vue/compiler-sfc": "^3.0.5",
    "eslint": "^7.25.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-vue": "^7.9.0",
    "typescript": "^4.1.3",
    "vite": "^2.2.3",
    "vue": "^3.0.5",
    "vue-tsc": "^0.0.24"
  },
  "engines": {
    "node": ">= 12.0.0",
    "npm": ">= 6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jekorx/vue-canvas-sign.git"
  },
  "bugs": {
    "url": "https://github.com/jekorx/vue-canvas-sign/issues"
  },
  "homepage": "https://github.com/jekorx/vue-canvas-sign#readme",
  "keywords": [
    "Vue",
    "Vue3",
    "vue-canvas-sign",
    "canvas",
    "canvas-sign",
    "sign"
  ],
  "license": "MIT"
}
